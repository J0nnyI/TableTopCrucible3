<Page x:Class="TableTopCrucible.App.WPF.Views.MainPageV"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TableTopCrucible.App.WPF.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPageV">
    <DockPanel>
        <ContentPresenter Content="{Binding Toolbar}"
                          DockPanel.Dock="Top"/>
        <ContentPresenter Content="{Binding JobOverview}"
                          DockPanel.Dock="Right"/>
        <Frame Content="{Binding LibraryPage}"/>
    </DockPanel>
</Page>
