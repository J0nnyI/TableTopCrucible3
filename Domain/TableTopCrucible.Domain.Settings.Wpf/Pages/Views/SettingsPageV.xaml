<rx:ReactiveUserControl x:Class="TableTopCrucible.Domain.Settings.Wpf.Pages.Views.SettingsPageV"
                        x:TypeArguments="pvm:SettingsPageVm"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                        xmlns:local="clr-namespace:TableTopCrucible.Domain.Settings.Wpf.Pages"
                        xmlns:rx="http://reactiveui.net"
                        xmlns:pvm="clr-namespace:TableTopCrucible.Domain.Settings.Wpf.Pages.ViewModels"
                        mc:Ignorable="d" 
                        d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Style="{StaticResource MaterialDesignHeadline1TextBlock}">Coming Soon</TextBlock>
        <Button Content="add jobs"
                Click="ButtonBase_OnClick"
                VerticalAlignment="Top" 
                HorizontalAlignment="Center"
                Grid.Row="1"/>
    </Grid>
</rx:ReactiveUserControl>
